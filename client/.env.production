# Production Environment Variables for React Frontend
# These will be set in AWS Amplify environment variables

# Backend API URL (Your Elastic Beanstalk URL)
REACT_APP_API_URL=https://your-eb-environment.elasticbeanstalk.com/api

# Giant Bomb API Configuration
REACT_APP_GIANT_BOMB_API_KEY=your-giant-bomb-api-key-here
REACT_APP_GIANT_BOMB_API_URL=https://www.giantbomb.com/api

# Build Configuration
CI=true
GENERATE_SOURCEMAP=false
